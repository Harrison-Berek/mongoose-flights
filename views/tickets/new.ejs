<%- include('../partials/header') %>

<h2>Tickets</h2>
<form method="POST"
  action="/flights/<%= flightId %>/tickets">
  <label>Seat</label>
  <input name="seat" required pattern="[A-F][1-9]\d?">
  <label>Price:</label>
  <input type="number" name="price">
  <input type="submit" required>
</form>

<%- include('../partials/footer') %>